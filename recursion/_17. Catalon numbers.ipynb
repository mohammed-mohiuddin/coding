   "outputs": [],
   "source": [
    "def catalon_number(n):\n",
    "    if n <= 1:\n",
    "        return 1\n",
    "    total = 0\n",
    "    for i in range(n):\n",
    "        total += catalon_number(n-i-1) * catalon_number(i)\n",
    "    return total"
   ]
  },
